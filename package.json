    "element-plus": "^2.4.3",
    "lodash-es": "^4.17.21",
    "pinia": "^2.1.7",
    "pinia-plugin-persist": "^1.0.0",
    "qs": "^6.11.2",
    "vue": "3.3.9",
    "vue-router": "^4.2.5",
    "vue-types": "^5.1.1",
    "web-storage-cache": "^1.1.1"
  },
